{
  "expo": {
    "name": "gojoin-app",
    "slug": "gojoin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.rkm414.gojoinapp",
      "runtimeVersion": "1.0.0"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "243e9bde-d107-43c5-85b7-feee401144f3"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/243e9bde-d107-43c5-85b7-feee401144f3"
    },
    "platforms": ["ios", "android", "web"]
  }
}
